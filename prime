#include <stdio.h>

int main()
{
int i,n,count=0;
printf("enter the number");
scanf("\n%d",&n);
for(i=1;i<=n;i++)
{
    if(n%i==0)
    {
        count++;
    }
}
    if(count==2)
    {
        printf("The number is prime");
    }
    else
    {
    printf("It is not prime");

}
}